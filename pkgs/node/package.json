{
    "name": "@re-/node",
    "version": "1.1.1",
    "author": "redo.dev",
    "description": "NodeJS utilities, build tools, and configs",
    "repository": {
        "type": "git",
        "url": "https://github.com/re-do/re-po.git",
        "directory": "pkgs/node"
    },
    "type": "module",
    "main": "out/cjs/index.cjs",
    "types": "out/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./out/types/index.d.ts",
            "import": "./out/esm/index.js",
            "require": "./out/cjs/index.cjs"
        }
    },
    "bin": {
        "runScript": "./runScript.js",
        "redoTsc": "./redoTsc.js",
        "redoTranspile": "./redoTranspile.js"
    },
    "scripts": {
        "build": "set TS_NODE_TRANSPILE_ONLY=1&& node -r ./filterWarnings.cjs --loader ts-node/esm ./src/redoTscCli.ts",
        "runScript": "./runScript.js",
        "redoTsc": "./redoTsc.js",
        "redoTranspile": "./redoTranspile.js",
        "test": "jest"
    },
    "dependencies": {
        "@re-/utils": "workspace:*",
        "deepmerge": "4.2.2",
        "execa": "5.0.0",
        "moize": "5.4.7",
        "prompts": "2.4.1",
        "ts-node": "10.4.0",
        "tree-kill": "1.2.2",
        "@jest/reporters": "27.4.2",
        "get-current-line": "6.6.0"
    },
    "peerDependencies": {
        "typescript": "^4.0.0"
    },
    "devDependencies": {
        "typescript": "4.5.2",
        "@types/node": "16.11.12",
        "@types/prompts": "2.0.11",
        "jest": "27.4.3",
        "@types/jest": "26.0.24",
        "@jest/types": "27.4.2",
        "ts-jest": "27.0.7",
        "vite": "2.4.4"
    }
}
