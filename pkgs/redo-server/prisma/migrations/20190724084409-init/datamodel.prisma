datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
  default  = true
}

generator photon {
  provider = "photonjs"
}

model BrowserEvent {
  id String @default(cuid()) @id @unique
  type String
  selector String
  value String
}

model Test {
  id String @default(cuid()) @id @unique
  name String
  tags String
  steps BrowserEvent
}

model User {
  id String @default(cuid()) @id @unique
  email String
  password String
  roles String
  firstName String
  lastName String
}

