{
    "name": "redo-server",
    "version": "1.0.0",
    "author": "Redo",
    "description": "Shared types that model Redo",
    "private": true,
    "scripts": {
        "build": "",
        "start": "npm run prisma && ts-node src/index.ts",
        "prisma": "npm run docker-start && npm run generate",
        "docker-start": "docker-compose -f src/database/docker-compose.yml up -d",
        "docker-stop": "docker-compose -f src/database/docker-compose.yml down",
        "generate": "npm run clean-generated && npm run generate-model && npm run prisma-deploy",
        "clean-generated": "rm -rf src/database/generated && mkdir src/database/generated",
        "generate-model": "ts-node --transpile-only generate.ts",
        "prisma-deploy": "cd src/database && prisma deploy",
        "prisma-force-deploy": "cd src/database && prisma deploy --force",
        "prisma-reset": "cd src/database && prisma reset",
        "test": "jest"
    },
    "dependencies": {
        "apollo-server": "2.5.0",
        "bcrypt": "3.0.6",
        "class-transformer": "0.2.0",
        "class-validator": "0.9.1",
        "deepmerge": "3.2.1",
        "dotenv": "7.0.0",
        "graphql": "14.4.2",
        "graphql-config": "2.2.1",
        "isomorphic-ws": "4.0.1",
        "jsonwebtoken": "8.5.0",
        "redo-model": "1.0.0",
        "reflect-metadata": "0.1.13",
        "type-graphql": "0.17.4"
    },
    "devDependencies": {
        "@types/bcrypt": "3.0.0",
        "@types/dotenv": "6.1.0",
        "@types/fs-extra": "8.0.0",
        "@types/graphql": "14.2.2",
        "@types/jest": "24.0.13",
        "@types/jsonwebtoken": "8.3.2",
        "@types/node": "12.6.6",
        "fs-extra": "8.1.0",
        "jest": "24.8.0",
        "prisma": "1.34.1",
        "redo-recommended": "1.0.0",
        "ts-jest": "24.0.2",
        "ts-node": "8.3.0",
        "typescript": "3.5.2"
    }
}
