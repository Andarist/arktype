{
    "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
    "rushVersion": "5.56.0",
    "pnpmVersion": "6.23.6",
    "pnpmOptions": {
        "useWorkspaces": true
    },
    "nodeSupportedVersionRange": ">=16.0.0",
    "suppressNodeLtsWarning": true,
    "ensureConsistentVersions": true,
    "repository": {
        "url": "https://github.com/re-do/dev",
        "defaultBranch": "main"
    },
    "projects": [
        {
            "packageName": "@re-/node",
            "projectFolder": "pkgs/node",
            "shouldPublish": true
        },
        {
            "packageName": "@re-/utils",
            "projectFolder": "pkgs/utils",
            "cyclicDependencyProjects": ["@re-/node", "@re-/assert"],
            "shouldPublish": true
        },
        {
            "packageName": "@re-/assert",
            "projectFolder": "pkgs/assert",
            "shouldPublish": true
        },
        {
            "packageName": "@re-/type",
            "projectFolder": "pkgs/type",
            "shouldPublish": true
        },
        {
            "packageName": "@re-/docs",
            "projectFolder": "pkgs/docs",
            "shouldPublish": false
        }
    ]
}
